(enum HTTP
      (Informational :status 100)
      (Successful :status 200)
      (Redirection :status 300)
      (ClientError :status 400)
      (ServerError :status 500))

(enum (HTTP100 HTTP)
      (Continue Informational :status 100)
      (SwitchingProtocol Informational :status 101))

(enum (HTTP200 HTTP)
      (OK Successful :status 200)
      (Created Successful :status 201)
      (Accepted Successful :status 202)
      (NonAuthoritativeInformation Successful :status 203)
      (NoContent Successful :status 204)
      (ResetContent Successful :status 205)
      (PartialContent Successful :status 206))

(enum (HTTP300 HTTP)
      (MultipleChoices Redirection :status 300)
      (MovedPermanently Redirection :status 301)
      (Found Redirection :status 302)
      (SeeOther Redirection :status 303)
      (NotModified Redirection :status 304)
      (UseProxy Redirection :status 305)
      (TemporaryRedirect Redirection :status 307))

(enum (HTTP400 HTTP)
      (BadRequest ClientError :status 400)
      (Unauthorized ClientError :status 401)
      (PaymentRequired ClientError :status 402)
      (Forbidden ClientError :status 403)
      (NotFound ClientError :status 404)
      (MethodNotAllowed ClientError :status 405)
      (NotAcceptable ClientError :status 406)
      (ProxyAuthenticationRequired ClientError :status 407)
      (RequestTimeout ClientError :status 408)
      (Conflict ClientError :status 409)
      (Gone ClientError :status 410)
      (LengthRequired ClientError :status 411)
      (PreconditionFailed ClientError :status 412)
      (RequestEntityTooLarge ClientError :status 413)
      (RequestURITooLong ClientError :status 414)
      (UnsupportedMediaType ClientError :status 415)
      (RequestedRangeNotSatisfiable ClientError :status 416)
      (ExpectationFailed ClientError :status 417))

(enum (HTTP500 HTTP)
      (InternalServerError ServerError :status 500)
      (NotImplemented ServerError :status 501)
      (BadGateway ServerError :status 502)
      (ServiceUnavailable ServerError :status 503)
      (GatewayTimeout ServerError :status 504)
      (HTTPVersionNotSupported ServerError :status 505))

(print (HTTP? Informational))
(print (Informational? Continue))
(print (Successful? NoContent))
(print (Successful? Continue))
(print (Redirection? Found))
(print (ClientError? Conflict))
(print (ServerError? BadGateway))
(print (HTTP500? NotImplemented))
(print (Continue.status))
(print (NotFound.status))
(print (Informational.status))
